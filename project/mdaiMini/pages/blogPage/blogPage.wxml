<view class="page">
   
    <view class="page__bd">
        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell">
                <view class="weui-cell__bd">
                    <view class="weui-uploader">
                        <view class="weui-uploader__bd">
                            <view class="weui-uploader__files">
                                <block  wx:for="{{galleryImg}}" wx:for-item="galleryImgs" wx:key="galleryImgs.id" >
                                    <view class="weui-uploader__file" >
                                        <image class="weui-uploader__img" src="https://m.daiplanet.com{{galleryImgs.image_show.url}}"  bindtap="previewImage" data-current="https://m.daiplanet.com{{galleryImgs.image_show.url}}" mode="aspectFill" style="width:100%;"/>
                                    </view>
                                </block>
                            </view>
                        </view>
                    </view>
                </view>
            </view>
        </view>
    </view>
</view>